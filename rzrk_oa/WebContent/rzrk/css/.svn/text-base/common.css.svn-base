body{font-size:12px;font-family:"微软雅黑", "Arial";}
a, .no-focus{outline: expression(this.hideFocus=true);outline: none;text-decoration:none;}
input, select{outline: none;}
.clear{clear:both;}
.clearfix:after {clear: both;content: ".";display: block;height: 0;visibility: hidden;}
.col-3-1{width:330px;overflow:hidden;}
.col-3-2{width:660px;overflow:hidden;}
.col-3-3{width:990px;overflow:hidden;}
.row-2-2{height:800px;overflow: hidden;}
/* header */
.header{width:990px;position:relative;margin:0 auto;z-index: 11;}
.header > ul > li{display:inline-block;margin-right:30px;position: relative;vertical-align: bottom;}
.header > ul > li > a{display:inline-block;cursor:pointer;width:100px;height:60px;line-height:60px;font-size:14px;color:#000;text-align:center;vertical-align:bottom;}
.header > ul > li.logo > a{width:90px;height:80px;line-height:80px;position: relative;overflow: hidden;}
/*.header > ul > li.logo > a{width:90px;height:80px;line-height:80px;background:#e0b500 url("../images/logo.png") 10px 35px no-repeat;}*/
.header > ul > li.logo > a > img{position: absolute;bottom: 5px;right: -20px;}
/*.header > ul > li.logo > a > span.text{position: absolute;bottom: -5px;right: 0;font-size: 12px;}*/
.header > ul > li.logo > a > span.text{position: absolute;bottom: 5px;right: 5px;font-size: 12px;line-height: 16px;line-height: 16px;text-align: right;}
.header > ul > li.logo > a > span.en{position: absolute;bottom: -27px;right: 0;font-size: 26px;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";filter:alpha(opacity=50);opacity: .5;}
.header > ul > li.logo > a > span.text, .header > ul > li.logo > a > span.en{display: none;}
.header > ul > li > a.active, .header > ul > li > a.selected, .header > ul > li > a:hover{background-color:#e0b500;color:#fff;}
.header .phone{position:absolute;right:5px;bottom:10px;}
.header .login, .header .logged{position:absolute;right:5px;top:20px;z-index: 1;}
.header .login .login-wrap, .header .logged .logged-wrap{position: relative;vertical-align: middle;}
.header .login a, .header .logged a.logout{color: #E0B500;height: 24px;line-height: 24px;display: inline-block;width: 44px;text-align: center;vertical-align: middle;}
.header .login a:hover, .header .logged a.logout:hover,.header .login a, .header .logged a.logout{color: #fff;background-color: #e0b500;}
.header .login a:active, .header .logged a.logout:active{color: #fff;background-color: #cfa42f;}
.header .login .login-wrap .input-panel{display: none;}
.header .login .login-wrap .input-panel input{border: 1px solid #dcdcdc;height: 22px;line-height: 22px;vertical-align: middle;color: #c9c9c9;margin-right: 5px;width: 120px;padding: 0 5px;}
.header .login .login-wrap .input-panel input.focus{border: 1px solid #cfa42f;}
.header .login .login-wrap .input-panel input.error{border: 1px solid #ea1300;}
.header .login .login-wrap .input-panel div{color: #666;position: absolute;width: 160px;padding: 10px;display: none;}
.header .login .login-wrap .input-panel div.id-card{background-color: #fffff4;top: 24px;left: 0;}
.header .login .login-wrap .input-panel div.pwd{background-color: #fffff4;top: 24px;left: 141px;}
.header .login .login-wrap .input-panel div.error{top: 24px;left: 0;color: #f00;border: 1px solid #f00;background-color: #ffeeee;}
.header .login .login-wrap a.close{display: none;height: 16px;width: 16px;position: absolute;right: 0;top: 4px;background: #FFF url("../images/close_s.png") 0 0 no-repeat;}
.header .login .login-wrap a.close:hover{background-position: -16px 0;}
.header .login .login-wrap a.close:active{background-position: -32px 0;}
.header .logged a.user{color: #4D90FE;height:24px; line-height: 24px;display: inline-block;text-align: center;vertical-align: middle;margin-right:10px;}
.header .logged a.user em{font-size: 10px;}
.header .logged span{vertical-align: middle;}
.header .logged{display: none;}
/* sub menu */
.header ul.sub-menu{position: absolute;z-index: 7;left: 0;top: 70px;display: none;}
.header ul.sub-menu li a{background-color:#e0b500;color:#fff;height: 30px;line-height: 30px;width:100px;font-size: 14px;display: inline-block;text-align: center;}
.header ul.sub-menu li a:hover{background-color: #f4cf00;}
/* user menu */
.header ul.user-menu{position: absolute;top: 25px;right:60px;display: none;}
.header ul.user-menu li a{background-color:#e0b500;color:#fff;height: 30px;line-height: 30px;width:80px;font-size: 12px;display: inline-block;text-align: center;}
.header ul.user-menu li a:hover{background-color: #f4cf00;}
/* footer */
.footer{width:980px;position:relative;margin:0 auto;padding:20px 5px;position:relative;}
.footer > a{margin-left:30px;color: #E0B500;}
.footer .partner{position:absolute;right:0;bottom:15px;}
.footer .partner > a{vertical-align: bottom;color:#fff;display:block;height:24px;line-height:24px;width:68px;text-align:center;background-color:#E0B500;}
.footer .partner > a.open{height:45px;line-height:66px;}
.footer .partner .logo-wrap{position:relative;}
.footer .partner .logo-wrap{padding: 50px 30px 30px;background-color: #fafafa;width: 930px;height: 238px;border-bottom: 2px solid #e0b500;position: absolute;bottom: 42px;right: 0;z-index: 6;display: none;}
.footer .partner .logo-panel{overflow:auto;height:220px;}
.footer .partner .logo-panel .logo{cursor: pointer;float: left;vertical-align: middle;height: 80px;line-height: 80px;position: relative;margin-bottom:30px;width: 101px;text-align: center;}
.footer .partner .logo-panel .logo img{}
.footer .partner .logo-panel .logo span{color: #666;text-align: center;height: 30px;line-height: 30px;display: inline-block;position: absolute;left: 0;bottom: -20px;width: 100%;}
.footer .partner .logo-wrap a.close{display: inline-block;height: 28px;width: 28px;position: absolute;right: 10px;top: 10px;background: #FFF url("../images/close.png") 0 0 no-repeat;}
.footer .partner .logo-wrap a.close:hover{background-position: -28px 0;}
.footer .partner .logo-wrap a.close:active{background-position: -56px 0;}
/* quick mark */
.quick-mark-panel{width: 990px;position: relative;margin: 0 auto;}
.quick-mark{width:140px; height: 244px;position: absolute;right: -160px;top: -202px;}
.quick-mark p{width:140px; text-align: center;color: #999;position: absolute;bottom: 15px;}
.quick-mark .close{width:18px; height:18px;background: transparent url(../images/quick-mark-close.jpg) 0 0 no-repeat;position: absolute;top:0;right:-18px;}
.quick-mark .close:hover{background-position: -18px 0;}
/* container */
.container{margin:10px auto 0;}
.container .top-border{border-top: 1px solid #e0b500;}
.container, .wrap, .wrap-remove{height:800px;width:990px;position:relative;overflow:hidden;}
.wrap, .wrap-remove{left:0;top:0;position:absolute;}
.wrap .nav-prev, .wrap .nav-next{display:none;}
.container .nav-prev,.container .nav-next{position:absolute;z-index:10;height:54px;line-height:54px;color:#fff;min-width:54px;overflow:hidden;background:#000 url("../images/icons.png") 0 0 no-repeat;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";filter:alpha(opacity=30);opacity: .3;}
.container .nav-prev{left:0;top:373px;background-position: left -3px;}
.container .nav-next{right:0;top:373px;background-position: right -3px;}
.container .nav-prev span, .container .nav-next span{}
.container .nav-prev:hover,.container .nav-next:hover{-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=85)";filter:alpha(opacity=85);opacity: .85;}
/*
.container .nav-prev:hover{padding:0 20px 0 54px;}
.container .nav-next:hover{padding:0 54px 0 20px;}
.container .nav-prev:hover{background-position: left bottom;padding:0 20px 0 54px;}
.container .nav-next:hover{background-position: right bottom;padding:0 54px 0 20px;}
*/
.container .nav-prev:hover{background-position: left -3px;padding:0 20px 0 54px;}
.container .nav-next:hover{background-position: right -3px;padding:0 54px 0 20px;}
.container .nav-prev span,.container .nav-next span{display:none;font-size:14px;font-weight:bold;}
.container .nav-prev:hover span,.container .nav-next:hover span{display:inline;}
/* home */
.container .banner{height:500px;width:990px;overflow:hidden;position:relative;color:#fff;float:left;position:relative;}
.container .banner img{width:990px;height:500px;}
.container .banner .chs-text{left:180px;top:90px;position:absolute;font-size:36px;z-index:10; display: none;}
.container .banner .en-text{left:150px;top:120px;position:absolute;font-size:36px;z-index:10;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=20)";filter:alpha(opacity=20);opacity: .2;display: none;}
.container .net-product-value, .introduction, .news{height:300px;float:left;position:relative;}
.container .net-product-value{background-color:#f5f5f5;}
.container .net-product-value h3{color:#fff;background-color:#e0b500;height:28px;line-height:28px;padding-left:10px;}
.container .net-product-value table{width:330px;margin-top:10px;table-layout:fixed;}
.container .net-product-value table thead tr{background-color:#ebebeb;}
.container .net-product-value table thead th{text-align:center;color:#999999;height:26px;}
.container .net-product-value table thead th:first-child{text-align:left;}
.container .net-product-value table thead th span{padding-left: 15px;}
.container .net-product-value table tbody tr{cursor: pointer;}
.container .net-product-value table tbody tr:hover{background-color:#FFF;}
.container .net-product-value table tbody td{text-align:center;color:#666666;height:37px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.container .net-product-value table tbody td:first-child{text-align:left;padding-left: 15px;}
.container .net-product-value table tbody td span.yellow{color:#d2a600;}
.container .net-product-value table tbody td span.red{color:#f00;}
.container .net-product-value table tbody td span.green{color:#0f0;}
.container .net-product-value table tbody td span.gray{color:#b9b9b9;}
.container .net-product-value .chart{height:100px;margin-top:3px;position: relative;}
.container .net-product-value .more{color:#fff;font-style:italic;float: right;margin-right: 10px;}
.container .net-product-value .preview-list{color:#fff;}
.container .net-product-value .more:hover, .container .net-product-value .preview-list:hover{text-decoration: underline;}
.container .introduction{background-color:#efefef;}
.container .introduction a{cursor:pointer;}
.container .introduction div{height:200px;overflow:hidden;}
.container .introduction p{width:300px;margin:10px auto 0;color:#666666;line-height:24px;}
.container .introduction img{width:330px;height:200px;}
.container .news{background-color:#f8fbf8;}
.container .news div{height:140px;overflow:hidden;}
.container .news .list{margin-top:15px;padding-left:20px;}
.container .news .list li{padding:5px 0;}
.container .news .list li a{color:#666666;display:inline-block;width:205px;vertical-align:middle;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.container .news a:hover{text-decoration:underline;}
.container .news .list li span{color:#b9b9b9;margin-left:20px;vertical-align:middle;}
.container .news .more-panel{text-align:right;padding:5px 20px;}
.container .news a.more{color:#E0B500;font-style:italic;}
/* product */
.container .products{width:220px;float:left;position:relative;background-color:#fafafa;}
.container .products > h3{height:30px;line-height:30px;/*background-color:#000;*/background-color:#e0b500;color:#fff;padding-left:20px;vertical-align: middle;}
.container .products .scroll{height: 766px;}
.container .products .scroll h4{padding: 10px 5px;background-color: #eee;font-weight: bold;}
/*.container .products .list{padding-top:10px;}*/
.container .products .list li{cursor:pointer;padding: 10px 15px;}
.container .products .list li:hover, .products .list li.selected{background-color:#eee;}
.container .products .list li span{display:block;overflow:hidden;text-overflow:ellipsis;margin: 0 0;font-weight: bold;color:#666;}
.container .products .list li span em{margin-left: 5px;}
.container .products .sub-menu > li{cursor:pointer;padding: 10px 30px;}
.container .products .sub-menu > li:hover{background-color:#eee;}
.container .products .sub-menu > li.selected{background-color:#eee;}
.container .products .sub-menu > li span{display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.container .product-banner, .container .product-banner-imgs{width:770px;height:400px;overflow:hidden;position:relative;z-index:1;}
.container .product-banner-imgs img{margin-left:-20px;}
.container .product-banner .content{position:absolute;top:60px;right:30px;z-index:6;width:390px;display:none;}
.container .product-banner .content h3{color:#f2bc00;font-size:30px;z-index: 6;}
.container .product-banner .content p{color:#666666;line-height:22px;z-index: 6;}
.container .product-banner .content .chart{width:200px;height:140px;margin-top:30px;z-index: 6;}
.container .product-banner .subscription{width: 100px;height: 38px;position: absolute;left: 13px;top:225px;bottom: 135px;cursor: pointer;z-index: 6;}
.container .product-idea{float:left;width:400px;height:400px;position:relative;overflow:hidden;background-color:#f5f5f5;z-index:1;}
.container .product-idea img{width:400px;height:270px;}
.container .product-idea p{margin:20px 30px 0;line-height: 22px;color:#666666;}
.container .product-other{float:left;width:370px;height:400px;overflow:hidden;position:relative;z-index:-1;background-color:#fafafa;}
.container .product-other img{width:378px;height:232px;}
.container .product-other p{color: #666;padding: 20px;line-height: 22px;}
/* about */
.container .about-banner{position: relative;float: left;}
.container .about-banner, .container .about-banner .banner-imgs, .container .about-banner .about-summary{height: 430px;}
.container .about-banner .banner-imgs{width: 774px;overflow: hidden;}
.container .about-banner .about-summary{width: 216px;position: absolute;top: 0;right: 0;line-height: 26px;background-color: #f5f5f5;}
.container .about-banner .about-summary h3{font-size: 26px;margin: 40px 10px 0;color: #666;}
.container .about-banner .about-summary p{margin: 10px 10px 0;color: #666;}
.container .a-idea{float:left;width:470px;height:370px;position:relative;overflow:hidden;background-color:#f3f3f3;}
.container .a-idea .idea-imgs{width:470px;height:230px;overflow: hidden;}
.container .a-idea .idea-description p, .container .a-system .system-description p{margin: 15px;line-height: 24px;color: #666;}
.container .a-idea .idea-description p span, .container .a-system .system-description p span{font-weight: bold;}
.container .a-system{float:left;width:520px;height:370px;position:relative;overflow:hidden;background-color:#f8fbf8;}
.container .a-system .system-imgs{width:520px;height:260px;overflow: hidden;}
/* service */
.container .service-banner{height:410px;width:990px;overflow:hidden;position:relative;color:#fff;float:left;}
.container .service-banner img{width:990px;height:410px;}
.container .service-banner .chs-text-yellow{right:285px;top:180px;position:absolute;font-size:42px;color: #e6c200;font-weight: bold;z-index:10;display:none;}
.container .service-banner .chs-text{right:45px;top:190px;position:absolute;font-size:32px;color: #747474;font-weight: bold;z-index:10;display:none;}
.s.pdf, .s.telephone, .s.net, .s.ie, .s.at, .s.meeting{height: 195px;position: relative;float: left;}
.s.ie{cursor: pointer;}
.s.pdf a, .s.at a, .s.meeting a{display: inline-block;height: 195px;width: 330px;cursor: pointer;}
.container .s.pdf{background: #fff url(../images/service/icon-pdf.png) 40px 50px no-repeat;}
.container .s.telephone{background: #fff url(../images/service/icon-telephone.png) 40px 50px no-repeat;}
.container .s.net{background: #fff url(../images/service/icon-net.png) 40px 50px no-repeat;}
.container .s.ie{background: #fff url(../images/service/icon-ie.png) 40px 50px no-repeat;}
.container .s.at{background: #fff url(../images/service/icon-at.png) 40px 50px no-repeat;}
.container .s.meeting{background: #fff url(../images/service/icon-meeting.png) 40px 50px no-repeat;}
.container .s h3{font-weight: bold;color: #666;position: absolute;left: 130px;top: 48px;}
.container .s p{color: #666;position: absolute;left: 130px;right: 20px;top:66px;line-height: 20px;}
/* info */
.container .info-affiche, .container .info-list{position: relative;height: 410px;float: left;}
.container .info-affiche .chs-text{font-size: 44px;color: #fff;position: absolute;left: 70px;bottom: 25px;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)";filter:alpha(opacity=70);opacity: .7;}
.container .info-list div{height: 260px;width: 330px;position: relative;}
.container .info-list div p{position: absolute;bottom: 0;left: 0;cursor: pointer;overflow: hidden;}
.container .info-list div p.bg{background-color: #000;height: 80px;width: 330px;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";filter:alpha(opacity=30);opacity: .3;}
.container .info-list div p.title{height: 50px;left:15px;right: 15px;bottom: 15px;display: inline-block;color: #fff;}
.container .info-list ul{height: 150px;padding: 15px 20px;background-color: #f5f5f5;}
.container .info-list ul li{line-height: 18px;}
.container .info-list ul li h3{font-weight: bold;font-size: 14px;margin-bottom: 10px;}
.container .info-list ul a{width: 290px;color: #666;display: inline-block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.container .info-list ul a:hover{text-decoration: underline;}
.container .info-1, .container .info-2{height: 390px;position: relative;float: left;}
.container .info-1{width: 560px;background-color: #f5f5f5;}
.container .info-1 .info-1-imgs{width: 560px;height: 306px;overflow: hidden;}
.container .info-1 p{height: 84px;position: relative;}
.container .info-1 p span{display: inline-block;position: absolute;top: 20px;left: 20px;bottom: 20px;right: 20px;overflow: hidden;color: #666;}
.container .info-2{width: 430px;background: #fafafa;}
.container .info-2 .info-2-imgs{width: 560px;height: 326px;overflow: hidden;}
.container .info-2 p{height: 64px;position: relative;}
.container .info-2 p span{display: inline-block;position: absolute;top: 15px;left: 15px;bottom: 15px;right: 15px;overflow: hidden;color: #666;}
/* statement */
.statement{color: #666;height: auto;min-height: 600px;}
.statement h3{text-align: center;color: #cfa42f;font-weight: bold;margin: 20px 0;font-size: 16px;}
.statement p{margin: 0 20px;line-height: 24px;}
.statement p em{color: #f00;}
.statement p.inscribe{text-align: right;margin-top: 20px;}
/* cooperation */
.cooperation{color: #666;height: auto;min-height: 600px;}
.cooperation h3{text-align: center;color: #cfa42f;font-weight: bold;margin: 20px 0;}
.cooperation p{margin: 0 20px;line-height: 24px;}
.cooperation p span{color: #cfa42f;font-size: 16px;}
.cooperation p a{color: #cfa42f;}
.cooperation p.inscribe{text-align: right;margin-top: 20px;}
